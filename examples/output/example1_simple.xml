<?xml version="1.0" encoding="UTF-8"?>
<bns:Component xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xmlns:bns="http://api.platform.boomi.com/"
               name="Simple Data Transform"
               type="process"
               folderName="Examples/Hybrid Architecture">
  <bns:encryptedValues/>
  <bns:description>Demonstrates hybrid architecture with linear flow</bns:description>
  <bns:object>
    <process xmlns=""
             allowSimultaneous="false"
             enableUserLog="false"
             processLogOnErrorOnly="false"
             purgeDataImmediately="false"
             updateRunDates="false"
             workload="general">
      <shapes>
        <shape image="start"
               name="start"
               shapetype="start"
               userlabel=""
               x="48.0"
               y="48.0">
          <configuration>
            <noaction/>
          </configuration>
          <dragpoints>
            <dragpoint name="start.dragpoint1"
                       toShape="transform_data"
                       x="224.0"
                       y="58.0"/>
          </dragpoints>
        </shape>
        <shape image="map_icon"
               name="transform_data"
               shapetype="map"
               userlabel="Transform customer data"
               x="240.0"
               y="48.0">
          <configuration>
            <map mapId="6c243379-108f-4bd7-ab7a-5a1055c43ba1"/>
          </configuration>
          <dragpoints>
            <dragpoint name="transform_data.dragpoint1"
                       toShape="end"
                       x="416.0"
                       y="58.0"/>
          </dragpoints>
        </shape>
        <shape image="returndocuments_icon"
               name="end"
               shapetype="returndocuments"
               userlabel=""
               x="432.0"
               y="48.0">
          <configuration>
            <returndocuments label=""/>
          </configuration>
          <dragpoints/>
        </shape>
      </shapes>
    </process>
  </bns:object>
  <bns:processOverrides/>
</bns:Component>